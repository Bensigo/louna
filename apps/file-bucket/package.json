{
    "name": "@solu/file-bucket",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "pnpm with-env next build",
        "clean": "git clean -xdf .next .turbo node_modules",
        "dev": "pnpm with-env next dev -p 6060",
        "lint": "dotenv -v SKIP_ENV_VALIDATION=1 next lint",
        "lint:fix": "pnpm lint --fix",
        "start": "pnpm with-env next start",
        "type-check": "tsc --noEmit",
        "with-env": "dotenv -e ../../.env --"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.484.0",
        "@aws-sdk/s3-presigned-post": "^3.484.0",
        "@aws-sdk/s3-request-presigner": "^3.484.0",
        "@solu/db": "^0.1.0",
        "aws-sdk": "^2.1529.0",
        "axios": "1.4.0",
        "micro": "^10.0.1",
        "next": "^13.4.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.43.9",
        "server": "link:@clerk/nextjs/server",
        "superjson": "1.9.1",
        "uuid": "^9.0.1",
        "zod": "^3.21.4",
        "zod-i18n-map": "^2.13.2"
    },
    "devDependencies": {
        "@prisma/nextjs-monorepo-workaround-plugin": "^5.6.0",
        "@solu/eslint-config": "^0.1.0",
        "@types/node": "^18.16.5",
        "@types/react": "^18.2.8",
        "@types/react-dom": "^18.2.4",
        "@types/uuid": "^9.0.7",
        "dotenv-cli": "^7.2.1",
        "eslint": "^8.42.0",
        "typescript": "^5.1.3"
    }
}
