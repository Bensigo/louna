        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://solu-web.vercel.app",
        "EXPO_PUBLIC_IMAGE_SERVER_URL": "https://solu-file-bucket.vercel.app",
        "EXPO_PUBLIC_AI_API_URL": "http://127.0.0.1:8000",
        "EXPO_PUBLIC_AI_API_X_SECRET":"tJC6XJ0Clpufo1VAPawbSYZXFfpeDw62"
