{
  "cli": {
    "version": ">= 3.3.0"
  },
  "build": {
    "base": {
      "node": "18.15.0",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_URL": "api.soluapp-dev.ae",
        "EXPO_PUBLIC_IMAGE_SERVER_URL": "image.soluappe-dev.ae"
      }

    },
    "preview": {
      "extends": "base",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_API_URL": "api.soluapp.ae",
        "EXPO_PUBLIC_IMAGE_SERVER_URL": "image.soluappe.ae"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
