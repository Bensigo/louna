      "env": {
        "EXPO_PUBLIC_API_URL": "api.soluapp.ae",
        "EXPO_PUBLIC_IMAGE_SERVER_URL": "image.soluappe.ae",
        "EXPO_PUBLIC_AI_API_URL": "http://127.0.0.1:8000",
        "EXPO_PUBLIC_AI_API_X_SECRET":"tJC6XJ0Clpufo1VAPawbSYZXFfpeDw62"
